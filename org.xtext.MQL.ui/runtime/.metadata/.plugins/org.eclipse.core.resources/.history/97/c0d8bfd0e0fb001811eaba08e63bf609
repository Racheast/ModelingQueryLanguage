package qrmm
import "/QRMM/model/QRMM.ecore"
		
pattern query(result:SearchableElement){
    find SystemUnitClass1267084827(result);
}

pattern a1_OrGroup(elem:Attribute){
    find Attribute1124543344(elem);
}

pattern Attribute1124543344(elem:Attribute){
    Attribute(elem);
    find n1_CAEXObject_name_AttrEClass(elem,n1);
    find v1_Attribute_value_AttrEClass(elem,v1);
    n1 != v1;
}

pattern SystemUnitClass1267084827(elem:SystemUnitClass){
    SystemUnitClass(elem);
    neg find a1_OrGroup_ref(elem,a1);
}

pattern n1_CAEXObject_name_AttrEClass(elem:Attribute,n1){
	Attribute.name(elem,n1);
	check(((((n1).length()))>(0)));
}

pattern v1_Attribute_value_AttrEClass(elem:Attribute,v1){
	Attribute.value(elem,v1);
	check(!((v1).isEmpty()));
}

pattern SystemUnitClass_attribute978184816(from:SystemUnitClass, to:Attribute){
    SystemUnitClass.attribute(from, to);
}

pattern a1_OrGroup_ref(container:SystemUnitClass, member:Attribute){
    find a1_OrGroup(member);
    find SystemUnitClass_attribute978184816(container,member);
}

